<:wrap admin/base.tmpl title=>[cond [ifMatch [cgi id] "extras"] [cat "Regenerate extras"] [cat "Regenerate site"]] nomessage=>"1" :>
<div class="window">
  <header>
    <h1><:param title:></h1>
    <a href="<:adminurl menu:>" accesskey="m" title="AccessKey = m" data-widget="close">Quick Launch</a>
  </header>
  <ul class="children list">
    <:iterator begin messages:>
    <:switch:>
    <:case RegenError:>
    <li class="error"><:message:></li>
    <:case Head:>
  </ul>
  <h2><:message:></h2>
  <ul class="children list">
    <:case default:>
    <li><:message:></li>
    <:endswitch:>
    <:iterator end messages:>
  </ul>
</div>